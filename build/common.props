<Project>
  <Import Project="sign.props" />
  <Import Project="version.props" />
  <PropertyGroup>
    <Authors>ihourglass</Authors>
    <Product>SpanableSkyAPM</Product>
    <owners>ihourglass</owners>
    <PackagePrefix>SpanableSkyAPM</PackagePrefix>
    <PackageIcon>icon.png</PackageIcon>
    <PackageProjectUrl>https://github.com/inversionhourglass/SkyAPM-dotnet</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/inversionhourglass/SkyAPM-dotnet/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/inversionhourglass/SkyAPM-dotnet</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>True</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>True</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>True</GenerateAssemblyProductAttribute>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
	  <None Include="../../icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>
